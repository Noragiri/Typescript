{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/js/${fileBasenameNoExtension}.js",
            "outFiles": [
                "${workspaceFolder}/js/**/*.js"
            ],
            "preLaunchTask": "Compile TypeScript"
        }
    ]
}
